{
  "name": "thumbnail-generator-api",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ali-A-Koye/Thumbnail-Generator-API.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ali-A-Koye/Thumbnail-Generator-API/issues"
  },
  "homepage": "https://github.com/Ali-A-Koye/Thumbnail-Generator-API#readme",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/env": "^4.1.0",
    "@fastify/sensible": "^5.0.0",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.4.1",
    "fastify-plugin": "4.2.1",
    "mongoose": "^6.5.4"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/tap": "^15.0.5",
    "ts-node": "^10.4.0",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "tap": "^16.1.0",
    "typescript": "^4.5.4"
  }
}
